{
    "buildCommand": "echo VERCEL_URL=$VERCEL_URL ; hugo --minify --baseURL=\"https://$VERCEL_URL/\"",
    "cleanUrls": true,
    "headers": [
        {
          "source": "/(.*)",
          "headers": [
            { "key": "Access-Control-Allow-Origin", "value": "*" }
          ]
        },
        {
          "source": "/vercel_app_domain_name/(.*)",
          "headers": [
            { "key": "Access-Control-Allow-Origin", "value": "*" }
          ]
        }
    ]  
}